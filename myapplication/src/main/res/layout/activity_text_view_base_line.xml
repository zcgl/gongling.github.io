<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_height="match_parent"
              android:layout_width="match_parent"
              android:orientation="vertical"
              android:background="#f9f9f9">

    <View android:layout_width="match_parent"
          android:layout_height="10dp"/>

    <TextView android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:background="#000000"
              android:textColor="#ffffff"
              android:text="我㎡是标题ing\n我㎡是标题ing"/>

    <View android:layout_width="match_parent"
          android:layout_height="10dp"/>

    <com.example.gongling.myapplication.page.WrapTextSingleLineView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:background="#000000"
              android:textColor="#ffffff"
              android:text="我㎡是标题ing\n我㎡是标题ing"/>

    <View android:layout_width="match_parent"
          android:layout_height="10dp"/>

    <com.example.gongling.myapplication.page.WrapTextSingleLineView
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:background="#000000"
            android:gravity="center"
            android:textColor="#ffffff"
            android:text="我㎡是标题ing\n我㎡是标题ing"/>

    <View android:layout_width="match_parent"
          android:layout_height="10dp"/>

    <com.example.gongling.myapplication.page.WrapTextMutiLinesView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#000000"
            android:textColor="#ffffff"
            android:lineSpacingExtra="0dp"
            android:lineSpacingMultiplier="1"
            android:text="我㎡是标题ing\n我㎡是标题ing\n我㎡是标题ing\n我㎡是标题ing"/>

    <View android:layout_width="match_parent"
          android:layout_height="10dp"/>


    <com.example.gongling.myapplication.page.WrapTextLinesView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#000000"
            android:textColor="#ffffff"
            android:text="我㎡是标题ing\n我㎡是标题ing"/>

    <com.example.gongling.myapplication.page.LinearSwipView
            android:layout_width="match_parent"
            android:gravity="center"
            android:layout_height="50dp">
        <TextView android:layout_width="wrap_content"
                  android:text="收到过佛山打工 "
                  android:layout_height="wrap_content"/>
    </com.example.gongling.myapplication.page.LinearSwipView>

</LinearLayout>
